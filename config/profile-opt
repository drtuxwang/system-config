if [ "$TERM" -a "$TERM" != dumb ]
then
    # Bash prompt over-ride for slow machines
    ##PS1='\[\033[01;32m\][\t] \[\033[01;34m\]\w\n\[\033[01;32m\]\u@\h\[\033[00m\] $ '

    # Re-enable writing of ".pyc" & ".pyo" files
    ##unset PYTHONDONTWRITEBYTECODE

    # All traffic to VPN
    ##export VPN_ALL_TRAFFIC=1

    # Python virtualenv activation regex
    [ ! "$VIRTUAL_ENV" ] && export PYTHON_VENVS=".*"
    ##export ANSIBLE_VERSION=2.12

    # API key files
    ##export ANSIBLE_KEY_FILE=
    ##export DROPBOX_KEY_FILE=

    # Directory change
    export cda="$HOME/Desktop/archive"
    export cdb=
    export cdc=
    export cdd="$HOME/Desktop/Downloads"
    export cde=
    export cdf="$HOME/Desktop/data"
    export cdg="$HOME/Desktop/git/system-config"
    export cdh="$HOME/Desktop"
    export cdi="$HOME/Desktop/images"
    export cdj=
    export cdk=
    export cdl="$HOME/Desktop/images/images-live/linux"
    export cdm="$HOME/Desktop/archive/software-main/dist"
    export cdn="$HOME/Desktop/archive/software-new/dist"
    export cdo="$HOME/Desktop/archive/software-old/dist"
    export cdp=
    export cdq="$HOME/Desktop/data/qemu"
    export cdr=
    export cds="$HOME/software"
    export cdt=
    export cdu="$HOME/Desktop/archive/software-new/pool/updates"
    export cdv="$HOME/Desktop/archive/data-video"
    export cdw="$HOME/Desktop/software/web-data/pages"
    export cdx=
    export cdy="$HOME/Desktop/private/yield"
    export cdz="$HOME/Desktop/git/drtuxwang.github.io"
fi

# Local Poetry
mkdir -p $HOME/.cache-local/pypoetry
ln -sf $HOME/.cache-local/pypoetry $HOME/.cache/pypoetry 2> /dev/null
